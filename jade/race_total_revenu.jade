include mixins

doctype html
html(lang='ru')
  +dev_head('Dynamica race total revenu', false, false, false, false, false, false, true)
  
  body
    .page.dashboard_page
      header.page-header.navbar.navbar-default
        .container-fluid.globalwidth
          .navbar-header
            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='.bs-navbar-collapse')
              span.icon-bar
              span.icon-bar
              span.icon-bar
            .navbar-project-switch
              a.navbar-brand(href='#') Dynamica
              span  of 
              .hover-select-box.front-select-btn
                select.selectpicker
                  option(value='Bling-Blings') Bling-Blings
                  option(value='Apple Store') Apple Store
                  option(value='Helvetica') Helvetica
                  
          nav.collapse.navbar-collapse.bs-navbar-collapse
            ul.nav.navbar-nav
              li.active
                a(href='projects.html') Projects
              li
                a(href='#') Team 
            ul.nav.navbar-right.hidden-xs
              li
                .user.dropdown
                  .arrow
                  a.user-toggle.dropdown-toggle(data-toggle='dropdown', href='#')
                    span.user-pic
                      img.img-circle(width='40', height='40', src='images/avatar1_40x40.jpg', alt='')
                    span.user-name
                      | Yaroslav Samoilov
                      i.icon.fa.fa-angle-down
                  .user-menu.dropdown-menu.right
                    ul
                      li
                        a(href='billing.html') Billing
                      li
                        a(href='profile.html') Profile
                      li
                        a(href='#') Help
                      li.clear
                        a(href='#') Log out
                        
      section.dashboard-body
        .container-fluid.globalwidth
          section.dashboard.projects-dashboard
            .row
              .col-sm-12
                +dashboard_panel()
                      
            .row.dashboard-title-row
              .col-sm-6: h2 Race Graph
              .col-sm-6
                .pull-right.datepicker-v1.datepickerComponent
                  .pull-left.datepicker-parent
                    input.form-control.datePicker.form-control-small(type='text')
                    
                  .pull-left
                    select.selectpicker.graphFilterDate
                      option(value=-1 data-hidden="true") 
                      option(value=0 selected) Current month
                      option(value=1) Previous month 
                      option(value=2) Last 3 month 
                      option(value=3) Last 6 month 
                      option(value=4) Last year 
                      option(value=5) All time 
                 
            .graph-unit-holder
              .graph-unit.dark_mod_
                .graph-value
                  span.val 42,489$
                  sup.graph-dynamica.dynamica_up +8%
                .graph-unit-large-v3
                  .graph-block
                    .areaChartTotal_1.hide_y_axis.hide_x_axis.d3graph.graph_3
                      #tooltip.graph-tooltip-holder.black-mod
                        #tooltip_content.graph-tooltip-top

              .legend-standalone.dark_mod_
                ul.legend_v2.graph-unit-legend.no_pointer
                  li.legend_item(data-graph='#family_area_0')
                    .legend_name_v2
                      span.color_red Current
                    .legend_val
                      span.val 35,489$
                      sup.graph-dynamica.dynamica_up +8%
                  li.legend_item(data-graph='#family_area_1')
                    .legend_name_v2
                      span Orders
                    .legend_val
                      span.val 490
                      sup.graph-dynamica.dynamica_down -9%
                  li.legend_item(data-graph='#family_area_2')
                    .legend_name_v2
                      span Products sell
                    .legend_val
                      span.val 9,483
                      sup.graph-dynamica.dynamica_down -9%
                  li.legend_item(data-graph='#family_area_3')
                    .legend_name_v2
                      span Unic users
                    .legend_val
                      span.val 109,330
                      sup.graph-dynamica.dynamica_down -1%
                  li.legend_item(data-graph='#family_area_4')
                    .legend_name_v2
                      span Customers
                    .legend_val
                      span.val 477
                      sup.graph-dynamica.dynamica_up +2
              
              .graph-unit-filter: form(action='#')
                .graph-filter-block 
                  .pull-left
                    ul.filter-btn-list
                      li 
                        label.btn-filter-check
                          input.hidden_input(type='radio' name='product_filter' checked)
                          span.btn.btn_v1.check_text All products
                      li 
                        label.btn-filter-check
                          input.hidden_input(type='radio' name='product_filter')
                          span.btn.btn_v1.check_text Hot sales
                      li 
                        label.btn-filter-check
                          input.hidden_input(type='radio' name='product_filter')
                          span.btn.btn_v1.check_text Last sales
                      li 
                        label.btn-filter-check
                          input.hidden_input(type='radio' name='product_filter')
                          span.btn.btn_v1.check_text Bestsellers
                  
                  .pull-right
                    .filter-search_block
                      input.form-control(placeholder='Product search')
                      button.filter-search-btn
                        i.fa.fa-search
                  
                .graph-unit-info
                  .graph-unit-info-table.scrollParent
                    .table-header.no_pointer
                      .table-cell.col_1
                        span Product
                      .table-cell.col_5.col_right
                        span Sales
                      .table-cell.col_6.col_right
                        span Revenu
                      .table-cell.col_7.col_right
                        span Price
                      .table-cell.col_8.col_right
                        span Views
                      .table-cell.col_9
                        span Category
                      .table-cell.col_4
                        span Date added
                    .table-footer.scrollBottomFixed
                      .table-cell.col_1 Total 49 products
                      .table-cell.col_5.col_right 658
                      .table-cell.col_6.col_right $3,998
                      .table-cell.col_7 &nbsp;
                      .table-cell.col_8 &nbsp;
                      .table-cell.col_9 &nbsp;
                      .table-cell.col_4 &nbsp;
                    .table-content
                      .table-row.col_wide
                        .wide-cell-content: .table-section-title 80% made of revenue
                      +loop_while(1)
                        .table-row
                          .table-cell.col_1.table-cell-best Wide Table Serif
                          .table-cell.col_5.col_right 38
                          .table-cell.col_6.col_right
                            .seller_val.best_seller $939
                          .table-cell.col_7.col_right $9
                          .table-cell.col_8.col_right 5,490
                          .table-cell.col_9 Kitchen
                          .table-cell.col_4 130$
                        .table-row.cell_border_btm
                          .table-cell.col_1.table-cell-best Wide Table Serif
                          .table-cell.col_5.col_right 38
                          .table-cell.col_6.col_right
                            .seller_val.best_seller $939
                          .table-cell.col_7.col_right $9
                          .table-cell.col_8.col_right 5,490
                          .table-cell.col_9 Kitchen
                          .table-cell.col_4 130$
                      .table-row.col_wide
                        .wide-cell-content: .table-section-title 20% made of revenue
                      +loop_while(1)
                        .table-row
                          .table-cell.col_1 Wide Table Serif
                          .table-cell.col_5.col_right 38
                          .table-cell.col_6.col_right
                            .seller_val $3,490
                          .table-cell.col_7.col_right $9
                          .table-cell.col_8.col_right 5,490
                          .table-cell.col_9 Kitchen
                          .table-cell.col_4 130$
                        .table-row
                          .table-cell.col_1 Wide Table Serif
                          .table-cell.col_5.col_right 38
                          .table-cell.col_6.col_right
                            .seller_val $3,490
                          .table-cell.col_7.col_right $9
                          .table-cell.col_8.col_right 5,490
                          .table-cell.col_9 Kitchen
                          .table-cell.col_4 130$
                      .table-row.no_hover
                        .table-cell.col_1
                          a.btn.btn_v3.check_text.graph-info-reload-btn(href='#')
                            i.fa.fa-refresh
                            span  Show 50 more products
                        .table-cell.col_5.col_right &nbsp;
                        .table-cell.col_6.col_right &nbsp;
                        .table-cell.col_7.col_right &nbsp;
                        .table-cell.col_8.col_right &nbsp;
                        .table-cell.col_9 &nbsp;
                        .table-cell.col_4 &nbsp;

                .graph-filter-block 
                  .pull-left
                    ul.filter-btn-list
                      li
                        a.btn.btn_v1.check_text(href='#')
                          span.arr_span ←
                          span &nbsp;Back
                      li
                        a.btn.btn_v1.check_text(href='#')
                          span Next&nbsp;
                          span.arr_span →
                      li.active
                        a.btn.btn_v2.pagination_btn(href='#') 1
                      li
                        a.btn.btn_v2.pagination_btn(href='#') 2
                      li
                        a.btn.btn_v2.pagination_btn(href='#') 3
                      li
                        a.btn.btn_v2.pagination_btn(href='#') 4
                      li
                        a.btn.btn_v2.pagination_btn(href='#') ...
                      li
                        a.btn.btn_v2.pagination_btn(href='#') 48
                      li
                        a.btn.btn_v2.pagination_btn(href='#') 49
                      li
                        a.btn.btn_v2.pagination_btn(href='#') 50
                  

      .page-footer
        .container-fluid.globalwidth
          ul.footer-menu
            li: a(href='#') Projects
            li: a(href='#') Team
            li: a(href='#') Profile
            li: a(href='#') Billing
            li: a(href='#') Support
            
          .footer-copyright All rights reserved © Osintsev. K.S. 2016
    .page-overlay.pageOverlay
      .jawn