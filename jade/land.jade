include mixins

doctype html
html.html_full_h(lang='ru')
    +dev_head('Dynamica land ', false, false, false, false, true)

    body.land_page
        // page 1
        .page.full_h_page
            header.page-header.navbar.navbar-default(
            data-0='padding: 50px 0 20px; background-color: rgba(255,255,255,1);'
            data-1000='padding: 10px 0 10px; background-color: rgba(255,255,255,.6);'
            data-6000='padding: 10px 0 10px; background-color: rgba(255,255,255,.6); transform: translate(0%,0%);'
            data-7000='padding: 10px 0 10px; background-color: rgba(255,255,255,.6); transform: translate(0%, -100%);')
                .container-fluid.globalwidth
                    .navbar-header
                        button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='.bs-navbar-collapse')
                            span.icon-bar
                            span.icon-bar
                            span.icon-bar
                        .navbar-project-switch
                            a.navbar-brand(href='#') Dynamica

                    nav.collapse.navbar-collapse.bs-navbar-collapse
                        ul.nav.navbar-right.hidden-xs.navbar-anon
                            li
                                a.menu_link(href='projects.html') Live Demo
                            li
                                a.menu_link(href='#') Pricing
                            li
                                a.btn.login_btn(href='#') Log in
            
            .abs_holder
                .gl_table: .gl_table_cell
                    section.land_hero(
                    data-0='top: 50%;'
                    data-1000='top: 50%;'
                    data-3000='top: -100%;')
                        .container-fluid.globalwidth
                            .row.dashboard-title-row.land_v1
                                .col-sm-12
                                    h1.text-center Carefully made analytics  <br> for your Shopify store
        
                                    .text-center
                                        a.btn.btn-lg.btn-danger.start_trial_btn(href='#') Start your free 30 day trial
    
                .hero_graph_block.heroGraph(
                data-0='transform: translate(0%,100%);'
                data-300='transform: translate(0%,0%);'
                data-1000='transform: translate(0%,0%);'
                data-2000='transform: translate(0%,-50%);'
                data-4000='transform: translate(0%,-500%);')
                    .graph_3.areaChart_2.d3graph.hide_x_axis(style='opacity: .2;')
                    .graph_3.areaChart_2.d3graph.hide_x_axis(style='opacity: .4;')
                    .graph_3.areaChart_2.d3graph.hide_x_axis
                    .graph_tip.g_tip_pink.graph_tip_1(
                    data-0='transform: translate(0%,-200%); opacity: 0;'
                    data-300='transform: translate(0%,-200%);opacity: 0;'
                    data-600='transform: translate(0%,-100%);opacity: 1;')
                        .g_tip_val $389
                        .g_tip_date Revenue today
                    .graph_tip.g_tip_orange.graph_tip_2(
                    data-0='transform: translate(0%,-200%); opacity: 0;'
                    data-300='transform: translate(0%,-200%);opacity: 0;'
                    data-600='transform: translate(0%,-100%);opacity: 1;')
                        .g_tip_val 43
                        .g_tip_date Sales 23 February

        // page 2
        .page
            .abs_holder.major_data_section(
            data-0='transform: translate(0%,100%);' 
            data-1000='transform: translate(0%,100%);' 
            data-3000='transform: translate(0%,-100%);' )
                .gl_table: .gl_table_cell
                    section(style='left: 50%;'
                    data-0='transform: translate(-50%,0%);'
                    data-1000='transform: translate(-50%,0%);'
                    data-6500='transform: translate(-50%,-100%);')
                        .container-fluid.globalwidth
                            .row.dashboard-title-row.land_v1
                                .col-sm-12
                                    h2.text-center All major data of your  <br>  store at single graph

                                    .graph-unit-holder: .graph-unit
                                        .graph-unit-large
                                            h5.graph-name Current status of your primary data
                                            .graph-filters.graph-filter-v1
                                                div(class="btn-group" role="group")
                                                    input#graph_filter_1(type='radio' hidden name='graph_filter' checked)
                                                    label(type="button" for='graph_filter_1' class="btn btn-default first") Day
                                                    input#graph_filter_2(type='radio' hidden name='graph_filter')
                                                    label(type="button" for='graph_filter_2' class="btn btn-default") Week
                                                    input#graph_filter_3(type='radio' hidden name='graph_filter')
                                                    label(type="button" for='graph_filter_3' class="btn btn-default last") Month

                                            .graph-block
                                                .graph_3.areaChartFamily_1.d3graph.hide_x_axis
                                                    #tooltip.graph-tooltip-holder.hidden_
                                                        #tooltip_dot.tooltip-dot
                                                        #tooltip_content.graph-tooltip

                                    .text-center
                                        a.hover_on_link(href='#') HOVER ON
        // page 3
        .page
            .abs_holder.major_data_section(
            data-0='transform: translate(0,100%);'  
            data-2000='transform: translate(0,100%);'
            data-3000='transform: translate(0%,0%);'
            data-6000='transform: translate(0%,0%);'
            data-7000='transform: translate(0,-100%);' )
                .gl_table: .gl_table_cell
                    section(style='left: 50%; '
                    data-0='transform: translate(-50%,100%); '
                    data-2000='transform: translate(-50%,100%);'
                    data-3000='transform: translate(-50%,0%);'
                    data-6000='transform: translate(-50%,0%);'
                    data-7000='transform: translate(-50%,-100%);')
                        .container-fluid.wide_block
                            .row.dashboard-title-row.land_v1
                                .col-sm-12
                                    h2.text-center(
                                    data-0='transform: translate(0%,300%); opacity: 0;'
                                    data-3400='transform: translate(0%,300%); opacity: 0;'
                                    data-3600='transform: translate(0%,0%); opacity: 1;'
                                    data-5600='transform: translate(0%,0%); opacity: 1;'
                                    data-6000='transform: translate(0%,-200%); opacity: 0;') Store overview
                                        
                                    .overview_block
                                        .overview_img
                                            img(src='img/Ipad.png')
                                        .overview_tip.ov_tip_left(
                                        data-0='transform: translate(0%,100%); opacity: 0;'
                                        data-3600='transform: translate(0%,100%); opacity: 0;'
                                        data-3800='transform: translate(0%,0%); opacity: 1;'
                                        data-5600='transform: translate(0%,0%); opacity: 1;'
                                        data-6000='transform: translate(0%,-100%); opacity: 0;')
                                            
                                            p Dashboard allow you to track store performance. Most important data about finances, customers and your products.
                                        .overview_tip.ov_tip_right(
                                        data-0='transform: translate(0%,100%); opacity: 0;'
                                        data-3800='transform: translate(0%,100%); opacity: 0;'
                                        data-4000='transform: translate(0%,0%); opacity: 1;'
                                        data-5600='transform: translate(0%,0%); opacity: 1;'
                                        data-6000='transform: translate(0%,-100%); opacity: 0;') 
                                            
                                            p Your will know your customers and how often they making orders.
                                            p Whether it’s new or returned users. Don’t forget to check out average sales.
        
        // page 4
        .page
            .abs_holder.major_data_section(
            data-0='transform: translate(0%,100%);'
            data-6000='transform: translate(0%,100%);'
            data-7000='transform: translate(0%,0%);'
            data-8000='transform: translate(0%,0%);'
            data-9000='transform: translate(0%,-100%);' )
                .gl_table: .gl_table_cell
                    section.major_data_section(style='left: 50%;'
                    data-0='transform: translate(-50%,100%);'
                    data-6000='transform: translate(-50%,100%);'
                    data-7000='transform: translate(-50%,0%);'
                    data-8000='transform: translate(-50%,0%);'
                    data-9000='transform: translate(-50%,-100%);')
                        .container-fluid.narrow_block(style='position: relative; z-index: 10;')
                            .row.dashboard-title-row.land_v1
                                .col-sm-12
                                    h2.text-center Most relevant metrics
                                        
                                    ul.relevant_list
                                        li
                                            .relevant_block
                                                .relevant_caption Finance
                                                ul.relevant_metrics
                                                    li Monthly Recurring 
                                                    li Net Revenue
                                                    li Average Revenue Per User
                                                    li Failed Charges
                                                    li Lifetime Value
                                        li
                                            .relevant_block
                                                .relevant_caption Customers
                                                ul.relevant_metrics
                                                    li Monthly Recurring 
                                                    li Net Revenue
                                                    li Average Revenue Per User
                                                    li Failed Charges
                                                    li Lifetime Value
                                        li
                                            .relevant_block
                                                .relevant_caption Products
                                                ul.relevant_metrics
                                                    li Monthly Recurring 
                                                    li Net Revenue
                                                    li Average Revenue Per User
                                                    li Failed Charges
                                                    li Lifetime Value

                        .container-fluid
                            .relevant_img(
                            data-0='transform: translate(0%,100%);'
                            data-7000='transform: translate(0%,100%);'
                            data-9000='transform: translate(0%,-100%);')
                                img(src='img/relevant_metrics.png')
                            
        // page 5
        .page
            .abs_holder.testimonials_section(
            data-0='transform: translate(0%,100%);'
            data-8000='transform: translate(0%,100%);'
            data-10000='transform: translate(0%,-100%);')
                .gl_table: .gl_table_cell
                    section(style='left: 50%;'
                    data-0='transform: translate(-50%,0%);'
                    data-9000='transform: translate(-50%,0%);'
                    data-10000='transform: translate(-50%,-100%);')
                        .container-fluid.wide_block
                            .row.dashboard-title-row.land_v1
                                .col-sm-12
                                    h2.text-center Testimonials
                                    
                                    .textimonials_slider_holder
                                        .swiper-container.testimonialSlider
                                            .swiper-wrapper
                                                +loop_while(6)
                                                    .swiper-slide
                                                        +testimon_slide()
                                        .swiper-controls
                                            .swiper-button-next.fa.fa-long-arrow-left
                                            .swiper-button-prev.fa.fa-long-arrow-right
                                                        
        // page 6
        .page
            .abs_holder.pricing_section(
            data-0='transform: translate(0%,100%);'
            data-9000='transform: translate(0%,100%);'
            data-10000='transform: translate(0%,0%);'
            data-13000='transform: translate(0%,0%);'
            data-14000='transform: translate(0%,-100%);')
                .gl_table: .gl_table_cell
                    section(
                    style='left: 50%;'
                    data-0='transform: translate(-50%,100%);'
                    data-9000='transform: translate(-50%,100%);'
                    data-10000='transform: translate(-50%,0%);'
                    data-13000='transform: translate(-50%,0%);'
                    data-14000='transform: translate(-50%,-100%);')
                        .container-fluid.wide_block
                            .row.dashboard-title-row.land_v1
                                .col-sm-12
                                    h2.text-center Pricing
                                    
                                    .pricing_block
                                        .pricing_img(
                                        data-0='transform: translate(-100%,0%); opacity: 0;'
                                        data-10000='transform: translate(-100%,0%); opacity: 0;'
                                        data-11000='transform: translate(0%,0%); opacity: 1;'
                                        data-12000='transform: translate(0%,0%); opacity: 1;'
                                        data-14000='transform: translate(0%,-100%); opacity: 1;')
                                            img(src='img/iPad_Pro_Smart_Keyboard.png')
                            
                                        .pricing_form(
                                        data-0='transform: translate(100%,0%); opacity: 0;'
                                        data-10500='transform: translate(100%,0%); opacity: 0;'
                                        data-11500='transform: translate(0%,0%); opacity: 1;'
                                        data-12500='transform: translate(0%,0%); opacity: 1;'
                                        data-13500='transform: translate(0%,-100%); opacity: 1;')
                                            .pricing_price $5
                                            .pricing_period month
                                            ul.pricing_features
                                                li Store metrics
                                                li Unlimited stores
                                                li Google analytics
                                                li Detailed Metrics
                                                li Support
                                            .pricing_controls
                                                a.btn.btn-lg.btn-danger.pricing_btn(href='#') Get started for free
                                            .pricing_share For annual payment  <br> 48$ per year

        // page 7
        .page
            .abs_holder.weekly_reporting_section(
            data-0='transform: translate(0%,100%);'
            data-13000='transform: translate(0%,100%);'
            data-14000='transform: translate(0%,0%);'
            data-16000='transform: translate(0%,0%);'
            data-17000='transform: translate(0%,-100%);')
                .gl_table: .gl_table_cell
                    section(
                    style='left: 50%;'
                    data-0='transform: translate(-50%,100%);'
                    data-12000='transform: translate(-50%,100%);'
                    data-13000='transform: translate(-50%,0%);'
                    data-16000='transform: translate(-50%,0%);'
                    data-17000='transform: translate(-50%,-100%);')
                        .container-fluid.wide_block
                            .row.dashboard-title-row.land_v1
                                .col-sm-12
                                    h2.text-center And also weekly  <br>  reporting 
                                    
                                    ul.report_block
                                        li(
                                        data-0='transform: translate(-100%,0%); opacity: 0;'
                                        data-13000='transform: translate(-100%,0%); opacity: 0;'
                                        data-14000='transform: translate(0%,0%); opacity: 1;'
                                        data-15000='transform: translate(0%,0%); opacity: 1;'
                                        data-16000='transform: translate(0%,-100%); opacity: 1;')
                                            .report_img
                                                img(src='img/report_1.png')
                                        li(
                                        data-0='transform: translate(100%,0%); opacity: 0;'
                                        data-13500='transform: translate(100%,0%); opacity: 0;'
                                        data-14500='transform: translate(0%,0%); opacity: 1;'
                                        data-15500='transform: translate(0%,0%); opacity: 1;'
                                        data-16500='transform: translate(0%,-100%); opacity: 1;')
                                            .report_img
                                                img(src='img/report_2.png')
        // page 8
        .page
            .abs_holder.demo_section(
            data-0='transform: translate(0%,100%);'
            data-16000='transform: translate(0%,100%);'
            data-17000='transform: translate(0%,0%);'
            data-19000='transform: translate(0%,0%);')
                .gl_table: .gl_table_cell
                    section(
                    style='left: 50%;'
                    data-0='transform: translate(-50%,100%);'
                    data-13000='transform: translate(-50%,100%);'
                    data-14000='transform: translate(-50%,0%);')
                        .container-fluid.wide_block
                            .row.dashboard-title-row.land_v1
                                .col-sm-12
                                    h2.text-center Look at demo at first
                                    .demo_controls
                                        a.btn.btn-lg.btn-danger.pricing_btn(href='#') Live demo

                .demo_block(
                data-0='transform: translate(0%,100%);'
                data-16500='transform: translate(0%,100%);'
                data-17500='transform: translate(0%,0%);')
                    img(src='img/Ipad.png')
                                        
                .page-footer
                    .container-fluid.wide_block
                        .footer_logo
                            a.navbar-brand(href='#') Dynamica
                            
                            
                        .land_footer-menu
                            ul
                                li: a(href='#') Tour
                                li: a(href='#') Demo
                                li: a(href='#') Pricing
                            ul
                                li: a(href='#') About
                                li: a(href='#') Blog
                                li: a(href='#') Support
                            ul
                                li: a(href='#') What's New
                                li: a(href='#') Terms
    
                        ul.footer_share
                            li
                                a.f_sh_link(href='#')
                                    i.fa.fa-facebook
