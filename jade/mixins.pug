        script(type='text/javascript', src='js/vendor/d3.min.js')
        script(type='text/javascript', src='js/vendor/bootstrap-select.min.js')
        script(type='text/javascript', src='js/vendor/moment.min.js')
        script(type='text/javascript', src='js/vendor/bootstrap-datepicker.js')
        script(type='text/javascript', src='js/script.js')
