mixin loop_while(counter)
    -i=0;
    while i < counter
        -i++;
        block
        
mixin testimon_slide()
    .testimonial_block
        .testimonial_text
            p Dynamica gives us incredible insights into our monthly numbers and growth, and provides us with invaluable information that allows us to improve our business in ways that weren't possible for us before.
        .testimonial_author
            img(src='img/testemonial_author.png')
        .testimonial_author_name Daniel Shultz  <br> Owner of Black Dark Shop

mixin popup_controls()
    .dev_popup_controls
        .dev_control
            button.btn.btn-info(type='button', data-toggle='modal', data-target='#auth_popup') auth_popup
        .dev_control
            button.btn.btn-info(type='button', data-toggle='modal', data-target='#restore_pass_popup') restore_pass_popup
        .dev_control
            button.btn.btn-info(type='button', data-toggle='modal', data-target='#no_access_popup') no_access_popup
        .dev_control
            button.btn.btn-info(type='button', data-toggle='modal', data-target='#not_ready_popup') not_ready_popup
        .dev_control
            button.btn.btn-info(type='button', data-toggle='modal', data-target='#confirm_address_popup') confirm_address_popup
        .dev_control
            button.btn.btn-info(type='button', data-toggle='modal', data-target='#welcome_popup') welcome_popup
        .dev_control
            button.btn.btn-info(type='button', data-toggle='modal', data-target='#account_expired_popup') account_expired_popup
        .dev_control
            button.btn.btn-info(type='button', data-toggle='modal', data-target='#billing_period_popup') billing_period_popup
        
mixin popups()
    #auth_popup.modal.fade.modal_vert_middle(role='dialog')
        .modal-dialog.modal_v1
            // Modal content
            .modal-content
                .modal-header
                    button.close.fa(type='button', data-dismiss='modal') ×
                    .modal-title
                        span.dynamica_loading
                        span Dynamica importing data from your Shopify
                .modal-body
                    h3 To see 
                        span.fw_700 Bling-Blings 
                        | analytics you need sign up
                    
                .modal-soc_auth
                    .control-box
                        a.btn-social-auth.btn-facebook(href='#')
                            i.icon.fa.fa-facebook
                            | Facebook
                    .control-box
                        a.btn-social-auth.btn-google(href='#')
                            i.icon.fa.fa-google-plus
                            | Google plus

                .form_holder.formHolder.error: form#auth_popup_form.validateMe(action='#')
                    .alternal_auth OR ENTER EMAIL MANUALY

                    .control-box.formCell
                        input.form-control(type='text', placeholder='Email' class='validate[required, custom[email]]')
                        
                    .control-box.formCell
                        input.form-control(type='password', placeholder='Password' class='validate[required, minSize[3]]')
                        
                    .control-box.text-center.btn_holder
                        button.btn.btn-lg.btn-danger.btn-disabled-if-error(type='submit') Go
                        
                    .control-box.text-center.btn_holder
                        a.btn-link.forgot_pass_link(href='#') I forgot password
                        
    #restore_pass_popup.modal.fade.modal_vert_middle(role='dialog')
        .modal-dialog.modal_v1
            // Modal content
            .modal-content
                .modal-header
                    button.close.fa(type='button', data-dismiss='modal') ×
                .modal-body
                    h3 Password recovery
 
                .form_holder.formHolder.error: form.validateMe(action='#')
                    .control-box
                        input.form-control(type='text', placeholder='Email' class='validate[required, custom[email]]')

                    .control-box.text-center.btn_holder
                        button.btn.btn-lg.btn-danger.btn-disabled-if-error(type='submit') Restore
                        
                    .control-box.text-center.btn_holder
                        a.btn-link.forgot_pass_link(href='#') Back
                                                                        
    #welcome_popup.modal.fade.modal_vert_middle(role='dialog')
        .modal-dialog.modal_v1(style='width: 600px;')
            // Modal content
            .modal-content
                .modal-header
                    button.close.fa(type='button', data-dismiss='modal') ×
                .modal-body
                    h3 Welcome to Dynamica!
                    
                    p.text-center
                        | We are happy to see you here, let’s link up your shop to start to analyze your store
 
                .form_holder: form(action='#')
                    .control-box.text-center.btn_holder
                        a.btn.btn-lg.btn-danger(type='submit') Link my store now

    #account_expired_popup.modal.fade.modal_vert_middle(role='dialog')
        .modal-dialog.modal_v1(style='width: 600px;')
            // Modal content
            .modal-content
                .modal-header
                    button.close.fa(type='button', data-dismiss='modal') ×
                .modal-body
                    h3 Your Dynamica account <br> has expired
                    
                    p.text-center
                        | Want to Re-Start you account?
 
                .form_holder: form(action='#')
                    .control-box.text-center
                        a.btn.btn-lg.btn-danger(type='submit') Re-Start Dynamica now!
                    .row.subscribe_block.mod_1: .col-lg-12
                        .pull-left
                            .subscribe_period: label.radio_control
                                input(type='radio' name='subscribe_period')
                                span.check_text $5 monthly
                            .subscribe_period: label.radio_control
                                input(type='radio' name='subscribe_period')
                                span.check_text $49 yearly, 
                                    span.color_green Save 20%
                        .pull-right.subscribe_switcher_w
                            span Charge from
                            .subscribe_switcher: .dropdown
                                button.btn.btn-default.dropdown-toggle(type='button', data-toggle='dropdown')
                                    | Bling-Blings account
                                    span.fa.fa-angle-down

                                ul.dropdown-menu.dropdown-menu-right
                                    li
                                        a(href='#') Bling-Blings account
                                    li
                                        a(href='#') Apple Store
                                    li
                                        a(href='#') Helvetica 
                                        
    #billing_period_popup.modal.fade.modal_vert_middle(role='dialog')
        .modal-dialog.modal_v1(style='width: 600px;')
            // Modal content
            .modal-content
                .modal-header
                    button.close.fa(type='button', data-dismiss='modal') ×
                .modal-body
                    h3 Change billing period  <br> or account
                     
                .form_holder: form(action='#')
                    .row.subscribe_block.mod_2: .col-lg-12
                        .pull-left
                            .subscribe_period: label.radio_control
                                input(type='radio' name='subscribe_period')
                                span.check_text $5 monthly
                            .subscribe_period: label.radio_control
                                input(type='radio' name='subscribe_period')
                                span.check_text $49 yearly, 
                                    span.color_green Save 20%
                        .pull-right.subscribe_switcher_w
                            span Charge from
                            .subscribe_switcher: .dropdown
                                button.btn.btn-default.dropdown-toggle(type='button', data-toggle='dropdown')
                                    | Bling-Blings account
                                    span.fa.fa-angle-down

                                ul.dropdown-menu.dropdown-menu-right
                                    li
                                        a(href='#') Bling-Blings account
                                    li
                                        a(href='#') Apple Store
                                    li
                                        a(href='#') Helvetica 
                                        
                    .control-box.text-center
                        a.btn.btn-lg.btn-danger(type='submit') Change
                                        
                    p.text-center.popup_tip Next payment will charged in 30 june
                        
    #no_access_popup.modal.fade.modal_vert_middle(role='dialog')
        .modal-dialog.modal_v1
            // Modal content
            .modal-content
                .modal-header
                    button.close.fa(type='button', data-dismiss='modal') ×
                    .modal-title &nbsp;
                .modal-body
                    h3 We didn’t get  <br> access to your Shopify  <br>  store, want to try again?
                     
                .form_holder
                    .control-box.text-center.btn_holder
                        a.btn.btn-lg.btn-danger(href='#') Yes, please
                        
                    .control-box.text-center.btn_holder
                        a.btn-link.forgot_pass_link(href='#') No
                                                
    #confirm_address_popup.modal.fade.modal_vert_middle(role='dialog')
        .modal-dialog.modal_v1
            // Modal content
            .modal-content
                .modal-header
                    button.close.fa(type='button', data-dismiss='modal') ×
                    .modal-title &nbsp;
                .modal-body.text-center
                    p Confirm your email address   <br> 
                        span.address yaosamo@gmail.com
                     
                .form_holder
                    .control-box.text-center.btn_holder
                        a.btn-link(href='#') Ok, thanks!
                        
                                                                        
    #not_ready_popup.modal.fade.modal_vert_middle(role='dialog')
        .modal-dialog.modal_v1(style='width: 728px;')
            // Modal content
            .modal-content
                .modal-header
                    button.close.fa(type='button', data-dismiss='modal') ×
                .modal-body
                    .modal_dynamica_logo.text-center
                        img(src='images/dynamica.svg')
                    
                    h3 Graphics are not ready yet,  <br> we can remind you
                     
                .form_holder
                    .control-box.text-center.btn_holder
                        a.btn.btn-lg.btn-danger(href='#') Remind me, on email
                        

