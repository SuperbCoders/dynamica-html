            script(type='text/javascript', src='js/dashboard_script.js')
        - if (total_revenu)
            script(type='text/javascript', src='js/total_revenu_script.js')
        - if (inside_total_revenu)
            script(type='text/javascript', src='js/inside_total_revenu_script.js')
